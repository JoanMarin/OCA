<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_phonecall_summary_view_form" model="ir.ui.view">
        <field name="model">crm.phonecall.summary</field>
        <field name="inherit_id" ref="crm_phonecall_summary_predefined.crm_phonecall_summary_view_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="is_agreement"/>
            </field>
        </field>
    </record>

    <record id="crm_phonecall_summary_view_tree" model="ir.ui.view">
        <field name="model">crm.phonecall.summary</field>
        <field name="inherit_id" ref="crm_phonecall_summary_predefined.crm_phonecall_summary_view_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="is_agreement"/>
            </field>
        </field>
    </record>
</odoo>
